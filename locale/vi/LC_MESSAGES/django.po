#: templates/registration/logged_out.html:5
msgid "Logged out!"
msgstr "Đã đăng xuất!"

#: templates/registration/logged_out.html:6
msgid "Click here to login again."
msgstr "Bấm vào đây để đăng nhập lại."

#: templates/registration/login.html:6
msgid "Your username and password didn't match. Please try again."
msgstr "Tên đăng nhập hoặc mật khẩu không đúng. Vui lòng thử lại."

#: templates/registration/login.html:14
msgid "Please login to see this page."
msgstr "Vui lòng đăng nhập để xem trang này."

#: templates/registration/login.html:31
msgid "Login"
msgstr "Đăng nhập"

#: templates/registration/login.html:36
msgid "Lost password?"
msgstr "Quên mật khẩu?"

#: templates/registration/password_reset_confirm.html:6
msgid "Please enter (and confirm) your new password."
msgstr "Vui lòng nhập (và xác nhận) mật khẩu mới của bạn."

#: templates/registration/password_reset_confirm.html:12
msgid "New password:"
msgstr "Mật khẩu mới:"

#: templates/registration/password_reset_confirm.html:18
msgid "Confirm password:"
msgstr "Xác nhận mật khẩu:"

#: templates/registration/password_reset_confirm.html:25
msgid "Change my password"
msgstr "Đổi mật khẩu"

#: templates/registration/password_reset_confirm.html:31
msgid "Password reset failed"
msgstr "Đặt lại mật khẩu không thành công"

#: templates/registration/password_reset_email.html:4
#, python-format
msgid ""
"Someone asked for a password reset for email %(email)s. Follow the link "
"below:"
msgstr ""
"Đã có người yêu cầu đặt lại mật khẩu cho email %(email)s. Hãy nhấp vào liên kết bên dưới:"

#: templates/registration/password_reset_form.html:14
msgid "Reset password"
msgstr "Đặt lại mật khẩu"
